@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Display Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td widh="50%">
                    @obj.Name
                </td>
                <td widh="25%">
                    @obj.DisplayOrder
                </td>
            </tr>
        }
    </tbody>
</table>